<templateSet group="AndroidLog">
  <template name="loge" value="android.util.Log.e(TAG, &quot;$METHOD_NAME$ &quot;+ AAAAA);" description="Log.e(TAG, String, Exception)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context />
  </template>
</templateSet>